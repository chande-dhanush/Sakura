{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for all windows",
  "windows": [
    "main",
    "bubble",
    "logs"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    {
      "identifier": "fs:allow-remove",
      "allow": [
        {
          "path": "$APPDATA/SakuraV10/audio/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-read",
      "allow": [
        {
          "path": "$APPDATA/SakuraV10/audio/**"
        }
      ]
    }
  ]
}